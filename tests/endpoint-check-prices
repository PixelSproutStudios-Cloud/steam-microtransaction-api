const express = require('express');
const app = express();
const prices = { item1001: 2.99, item1002: 4.99, item1003: 0.99 };

app.get('/prices', (req, res) => {
    res.json(prices);
});

const port = process.env.PORT || 3000;
app.listen(port, () => {
    console.log(`Server is running on port ${port}`);
});
